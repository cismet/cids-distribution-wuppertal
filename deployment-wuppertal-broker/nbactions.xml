<?xml version="1.0" encoding="UTF-8"?>
<actions>
    <action>
        <actionName>build</actionName>
        <packagings>
            <packaging>*</packaging>
        </packagings>
        <goals>
            <goal>install</goal>
        </goals>
        <activatedProfiles>
            <activatedProfile>netbeans-fastbuild</activatedProfile>
        </activatedProfiles>
    </action>
    <action>
        <actionName>clean</actionName>
        <packagings>
            <packaging>*</packaging>
        </packagings>
        <goals>
            <goal>clean</goal>
        </goals>
        <activatedProfiles>
            <activatedProfile>netbeans-fastbuild</activatedProfile>
        </activatedProfiles>
    </action>
    <action>
        <actionName>test</actionName>
        <packagings>
            <packaging>*</packaging>
        </packagings>
        <goals>
            <goal>test</goal>
        </goals>
        <activatedProfiles>
            <activatedProfile>netbeans-fastbuild-test</activatedProfile>
        </activatedProfiles>
    </action>
    <action>
        <actionName>test.single</actionName>
        <packagings>
            <packaging>*</packaging>
        </packagings>
        <goals>
            <goal>test-compile</goal>
            <goal>surefire:test</goal>
        </goals>
        <properties>
            <test>${packageClassName}</test>
        </properties>
        <activatedProfiles>
            <activatedProfile>netbeans-fastbuild-test</activatedProfile>
        </activatedProfiles>
    </action>
    <action>
        <actionName>run.single.main</actionName>
        <packagings>
            <packaging>*</packaging>
        </packagings>
        <goals>
            <goal>process-classes</goal>
            <goal>org.codehaus.mojo:exec-maven-plugin:exec</goal>
        </goals>
        <properties>
            <exec.args>-classpath %classpath ${packageClassName}</exec.args>
            <exec.executable>java</exec.executable>
            <exec.classpathScope>${classPathScope}</exec.classpathScope>
            <mdep.skip>true</mdep.skip>
            <cids.generate-lib.skip>true</cids.generate-lib.skip>
            <exec.skip>false</exec.skip>
        </properties>
        <activatedProfiles>
            <activatedProfile>netbeans-fastbuild</activatedProfile>
        </activatedProfiles>
    </action>
    <action>
        <actionName>debug.single.main</actionName>
        <packagings>
            <packaging>*</packaging>
        </packagings>
        <goals>
            <goal>process-classes</goal>
            <goal>org.codehaus.mojo:exec-maven-plugin:exec</goal>
        </goals>
        <properties>
            <exec.args>-Xdebug -Xrunjdwp:transport=dt_socket,server=n,address=${jpda.address} -classpath %classpath ${packageClassName}</exec.args>
            <exec.executable>java</exec.executable>
            <exec.classpathScope>${classPathScope}</exec.classpathScope>
            <jpda.listen>true</jpda.listen>
            <mdep.skip>true</mdep.skip>
            <cids.generate-lib.skip>true</cids.generate-lib.skip>
            <exec.skip>false</exec.skip>
        </properties>
        <activatedProfiles>
            <activatedProfile>netbeans-fastbuild</activatedProfile>
        </activatedProfiles>
    </action>
    <action>
        <actionName>debug.test.single</actionName>
        <packagings>
            <packaging>*</packaging>
        </packagings>
        <goals>
            <goal>test-compile</goal>
            <goal>surefire:test</goal>
        </goals>
        <properties>
            <test>${packageClassName}</test>
            <forkMode>once</forkMode>
            <maven.surefire.debug>-Xdebug -Xrunjdwp:transport=dt_socket,server=n,address=${jpda.address}</maven.surefire.debug>
            <jpda.listen>true</jpda.listen>
        </properties>
        <activatedProfiles>
            <activatedProfile>netbeans-fastbuild-test</activatedProfile>
        </activatedProfiles>
    </action>
    <action>
        <actionName>profile.single.main</actionName>
        <packagings>
            <packaging>*</packaging>
        </packagings>
        <goals>
            <goal>process-classes</goal>
            <goal>org.codehaus.mojo:exec-maven-plugin:exec</goal>
        </goals>
        <properties>
            <exec.args>-classpath %classpath ${packageClassName}</exec.args>
            <exec.executable>java</exec.executable>
            <exec.classpathScope>${classPathScope}</exec.classpathScope>
            <mdep.skip>true</mdep.skip>
            <cids.generate-lib.skip>true</cids.generate-lib.skip>
            <exec.skip>false</exec.skip>
        </properties>
        <activatedProfiles>
            <activatedProfile>netbeans-fastbuild</activatedProfile>
        </activatedProfiles>
    </action>
    <action>
        <actionName>javadoc</actionName>
        <packagings>
            <packaging>*</packaging>
        </packagings>
        <goals>
            <goal>generate-sources</goal>
            <goal>javadoc:javadoc</goal>
        </goals>
        <activatedProfiles>
            <activatedProfile>netbeans-fastbuild</activatedProfile>
        </activatedProfiles>
    </action>
    <action>
        <actionName>CUSTOM-run</actionName>
        <displayName>run</displayName>
        <packagings>
            <packaging>jar</packaging>
        </packagings>
        <goals>
            <goal>process-classes</goal>
            <goal>org.codehaus.mojo:exec-maven-plugin:exec</goal>
        </goals>
        <properties>
            <exec.args>-classpath %classpath Sirius.server.middleware.impls.proxy.StartProxy runtime.properties</exec.args>
            <exec.workingdir>${de.cismet.cids.server.workingDir.wunda}</exec.workingdir>
            <exec.executable>java</exec.executable>
            <mdep.skip>true</mdep.skip>
            <cids.generate-lib.skip>true</cids.generate-lib.skip>
            <exec.skip>false</exec.skip>
        </properties>
        <activatedProfiles>
            <activatedProfile>netbeans-fastbuild</activatedProfile>
        </activatedProfiles>
    </action>
    <action>
        <actionName>CUSTOM-run-serverconsole</actionName>
        <displayName>run with serverconsole</displayName>
        <packagings>
            <packaging>jar</packaging>
        </packagings>
        <goals>
            <goal>process-classes</goal>
            <goal>org.codehaus.mojo:exec-maven-plugin:exec</goal>
        </goals>
        <properties>
            <exec.args>-classpath %classpath de.cismet.cids.admin.serverManagement.HeadlessServerConsole runtime.properties</exec.args>
            <exec.workingdir>${de.cismet.cids.server.workingDir.wunda}</exec.workingdir>
            <exec.executable>java</exec.executable>
            <mdep.skip>true</mdep.skip>
            <cids.generate-lib.skip>true</cids.generate-lib.skip>
            <exec.skip>false</exec.skip>
        </properties>
        <activatedProfiles>
            <activatedProfile>netbeans-fastbuild</activatedProfile>
        </activatedProfiles>
    </action>
    <action>
        <actionName>CUSTOM-debug</actionName>
        <displayName>debug</displayName>
        <packagings>
            <packaging>jar</packaging>
        </packagings>
        <goals>
            <goal>process-classes</goal>
            <goal>org.codehaus.mojo:exec-maven-plugin:exec</goal>
        </goals>
        <properties>
            <exec.args>-Xdebug -Xrunjdwp:transport=dt_socket,server=n,address=${jpda.address} -classpath %classpath Sirius.server.middleware.impls.proxy.StartProxy runtime.properties</exec.args>
            <exec.executable>java</exec.executable>
            <exec.workingdir>${de.cismet.cids.server.workingDir.wunda}</exec.workingdir>
            <jpda.listen>true</jpda.listen>
            <mdep.skip>true</mdep.skip>
            <cids.generate-lib.skip>true</cids.generate-lib.skip>
            <exec.skip>false</exec.skip>
        </properties>
        <activatedProfiles>
            <activatedProfile>netbeans-fastbuild</activatedProfile>
        </activatedProfiles>
    </action>
    <action>
        <actionName>CUSTOM-profile</actionName>
        <displayName>profile</displayName>
        <packagings>
            <packaging>jar</packaging>
        </packagings>
        <goals>
            <goal>process-classes</goal>
            <goal>org.codehaus.mojo:exec-maven-plugin:exec</goal>
        </goals>
        <properties>
            <exec.args>${profiler.args} -classpath %classpath Sirius.server.middleware.impls.proxy.StartProxy runtime.properties</exec.args>
            <exec.workingdir>${de.cismet.cids.server.workingDir.wunda}</exec.workingdir>
            <exec.executable>${profiler.java}</exec.executable>
            <mdep.skip>true</mdep.skip>
            <cids.generate-lib.skip>true</cids.generate-lib.skip>
            <exec.skip>false</exec.skip>
        </properties>
        <activatedProfiles>
            <activatedProfile>netbeans-fastbuild</activatedProfile>
        </activatedProfiles>
    </action>
    <action>
        <actionName>CUSTOM-registry</actionName>
        <displayName>run registry</displayName>
        <goals>
            <goal>process-classes</goal>
            <goal>org.codehaus.mojo:exec-maven-plugin:exec</goal>
        </goals>
        <activatedProfiles>
            <activatedProfile>netbeans-fastbuild</activatedProfile>
        </activatedProfiles>
        <properties>
            <exec.classpathScope>runtime</exec.classpathScope>
            <exec.workingdir>${de.cismet.cids.server.workingDir.registry}</exec.workingdir>
            <exec.args>-Xdebug -Xrunjdwp:transport=dt_socket,server=y,address=8996,suspend=n -Xmx1024m -classpath %classpath de.cismet.cids.admin.serverManagement.HeadlessServerConsole runtime.properties</exec.args>
            <exec.executable>java</exec.executable>
            <mdep.skip>true</mdep.skip>
            <cids.generate-lib.skip>true</cids.generate-lib.skip>
            <exec.skip>false</exec.skip>
        </properties>
    </action>
    <action>
        <actionName>CUSTOM-registry-serverconsole</actionName>
        <displayName>run registry with serverconsole</displayName>
        <goals>
            <goal>process-classes</goal>
            <goal>org.codehaus.mojo:exec-maven-plugin:exec</goal>
        </goals>
        <activatedProfiles>
            <activatedProfile>netbeans-fastbuild</activatedProfile>
        </activatedProfiles>
        <properties>
            <exec.classpathScope>runtime</exec.classpathScope>
            <exec.workingdir>${de.cismet.cids.server.workingDir.registry}</exec.workingdir>
            <exec.args>-Xdebug -Xrunjdwp:transport=dt_socket,server=y,address=8996,suspend=n -Xmx1024m -classpath %classpath Sirius.server.registry.Registry runtime.properties</exec.args>
            <exec.executable>java</exec.executable>
            <mdep.skip>true</mdep.skip>
            <cids.generate-lib.skip>true</cids.generate-lib.skip>
            <exec.skip>false</exec.skip>
        </properties>
    </action>
</actions>
